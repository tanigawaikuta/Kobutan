<?xml version="1.0"?>
<CommanderSetting xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Information>
    <Name>KobukiSample3</Name>
    <Version>1.0</Version>
    <BaseCommander></BaseCommander>
    <Description>Kobukiサンプル3。ボタン0が押されていればLEDを付け、押されてなければLEDを消す。</Description>
  </Information>
  <Communication>
    <Protocol>Kobuki</Protocol>
    <SendThreadEnable>true</SendThreadEnable>
    <SendCycle>10</SendCycle>
  </Communication>
  <Programming>
    <Language>CSharp</Language>
    <WithComplement>true</WithComplement>
  </Programming>
</CommanderSetting>